<?xml version='1.0' encoding='utf-8'?>
<widget id="com.earthoracles.celtmistic" version="1.0.6"
        xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>The Talismans</name>
    <description>
        Celtic talismans to protect, persuade, and bring luck.
    </description>
    <author email="admin@koalatea-software.com" href="http://koalateasoftware.com">
        Koala Tea Software
    </author>
    <content src="index.html" />
    <plugin name="cordova-plugin-whitelist" spec="1" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />

    <preference name="android-minSdkVersion" value="28"/>
    <preference name="android-targetSdkVersion" value="28"/>

    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
    <plugin name="cc.fovea.cordova.purchase" spec="^7.4.1">
        <variable name="BILLING_KEY"
                  value="" />
    </plugin>
    <engine name="android" spec="^7.1.4" />
</widget>
